[run]
relative_files = true
branch = true
omit = */settings.py, */tests/*, */migrations/*, */urls.py, */settings/*
    */wsgi.py, */asgi.py, manage.py, fabfile.py, app/local_settings.py, */apps.py, populate.py
    setup.py
source = livewire

[report]
show_missing = true
skip_covered = true
exclude_lines = 
    def __str__
